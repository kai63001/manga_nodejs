    <meta name="keywords"
        content="<%= name.replace(/-/g,' '); %> ,อ่านการ์ตูน <%= name.replace(/-/g,' '); %> , เว็บอ่านการ์ตูน <%= name.replace(/-/g,' '); %> , มังงะ <%= name.replace(/-/g,' '); %> , มังงะแปลไทย,manga <%= name.replace(/-/g,' '); %> ,อ่านการ์ตูนออนไลน์, แอพอ่านการ์ตูน, มือถือ,อ่านโทรศัพย์, อ่านการ์ตูน,มังฮวา ">
    <link rel="icon" href="https://www.moonsmanga.com/views/views/favicon.ico">
    <link rel="apple-touch-icon" href="https://www.moonsmanga.com/views/views/favicon.ico">
    <link rel="apple-touch-icon-precomposed" href="https://www.moonsmanga.com/views/views/favicon.ico">

    <meta name="robots" content="noodp,noydir,index,follow,noarchive" />
    <meta property="og:locale" content="th_TH" />
    <meta property="og:type" content="website" />
    <meta property="og:site_name" content="www.moonsmanga.com" />
    <meta property="article:publisher" content="https://www.facebook.com/box.manga.page" />
    <meta property="fb:app_id" content="153313228407799" />

    <meta property="og:title"
        content="อ่านการ์ตูน เว็บอ่านการ์ตูน อ่านการ์ตูนออนไลน์ อ่านมังงะ manga แปลไทย - moonsmanga.com" />
    <meta property="og:description"
        content="MoonsManga เว็บอ่านการ์ตูนออนไลน์ อ่านมังงะ manga แปลไทย app แอพอ่านการ์ตูน android ios iphone ipad การ์ตูนอัพเดทใหม่ทุกวัน" />
    <meta property="og:url" content="https://www.moonsmanga.com/" />
    <meta property="og:image" content="http://www.moonsmanga.com/favicon.png" />
    <link rel="shortcut icon" type="image/png" href="/favicon.png" />
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css"
        integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
    <link href="https://fonts.googleapis.com/css?family=Mitr&display=swap" rel="stylesheet">
    <%- header %> 

    <style>
        .card:hover {
            background-size: 120% !important;
        }
    </style>
    <style>
        .download-btn {
            position: absolute;
            top: 5px;
            right: 25px;
            color: white;
            background-color: black;
            padding: 3px 9px;
            border-radius: 3px;
            opacity: 0;
            transition: 0.5s;
            border: 0px;
            text-decoration: none;
        }

        .download-btn:hover {
            color: black;
            background-color: white;
        }

        .btngro:hover .download-btn {
            opacity: 1;
        }

        .page-item a {
            position: relative;
            display: block;
            padding: .5rem .75rem;
            margin-left: -1px;
            line-height: 1.25;
            color: #343a40;
            background-color: #fff;
            border: 1px solid #dee2e6;
            text-decoration: none;

        }

        .activepagi a {
            text-decoration: none;
            background-color: #343a40 !important;
            color: white;
            border-color: #343a40 !important;
        }
    </style>
</head>


<body style="font-family: 'Mitr' !important;">
    <%- include('../include/nav_index') %>
    <div class="container">
        <div class="row">
            <% for( let i = 0; i < tags.length; i++ ) { %>
            <div class="col-md-2 col-6" style="padding-right: 0px;">
                <a href="/manga/<%= tags[i].md_url.replace('/cumanga.com/manga-',''); %>"
                    style="text-decoration: none;color: #130C11;">
                    <% if (tags[i].md_image != '') { %>
                    <div class="card"
                        style="background:url('<%= tags[i].md_image %>');background-size: cover;background-position: center; height: 240px;width: 100%;border-radius: 4px;border:0px;">
                    </div>
                    <% } else { %>
                    <div class="card"
                        style="background:url('card.png');background-size: cover;background-position: center; height: 240px;width: 100%;border-radius: 4px;border:0px;">
                    </div>
                    <% } %>

                    <p style="margin-top: 5px;white-space: nowrap;overflow: hidden;text-overflow: ellipsis;">
                        <strong> <%= tags[i].md_name %> </strong><br>

                    </p>
                </a>
            </div>

            <% } %>

        </div>
    </div>
    <br>
    <ul id="pagination-demo" class="justify-content-center">
    </ul>
    <br>
    <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js"
        integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo"
        crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js"
        integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6"
        crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/mobile-detect/1.4.4/mobile-detect.min.js"></script>
    <script
        src="https://www.jqueryscript.net/demo/Simple-Customizable-Pagination-Plugin-with-jQuery-Bootstrap-Twbs-Pagination/jquery.twbsPagination.js"></script>
    <script>
        $('#pagination-demo').twbsPagination({
            totalPages: parseInt('<%= lastPage%>'),
            startPage: 1,
            visiblePages: 5,
            initiateStartPageClick: true,
            hideOnlyOnePage: true,
            href: '?page={{number}}',
            pageVariable: '{{page}}',
            totalPagesVariable: '{{total_pages}}',
            page: null,
            first: '',
            prev: 'Previous',
            next: 'Next',
            last: '',
            loop: false,
            onPageClick: null,

            paginationClass: 'pagination',
            nextClass: 'page-item next',
            prevClass: 'page-item prev',
            lastClass: 'page-item last',
            firstClass: 'page-item first',
            pageClass: 'page-item',
            activeClass: 'activepagi',
            disabledClass: 'disabled',
            anchorClass: 'page-link',
        });

    </script>
    <%- include('../include/footer') %>
    <%- footer %> 

</body>

</html>